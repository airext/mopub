<extension xmlns="http://ns.adobe.com/air/extension/16.0">
  <id>com.sticksports.nativeExtensions.mopub.admob</id>
  <versionNumber>0.0.1</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdMobSupport.a</nativeLibrary>
        <initializer>ANXMoPubAdMobInitializer</initializer>
        <finalizer>ANXMoPubAdMobFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdMobSupportSimulator.a</nativeLibrary>
        <initializer>ANXMoPubAdMobInitializer</initializer>
        <finalizer>ANXMoPubAdMobFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdMobSupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.admob.MoPubAdMobSupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.admob.MoPubAdMobSupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdMobSupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.admob.MoPubAdMobSupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.admob.MoPubAdMobSupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>
