<extension xmlns="http://ns.adobe.com/air/extension/16.0">
  <id>com.sticksports.nativeExtensions.mopub.adcolony</id>
  <versionNumber>0.9.6</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdColonySupport.a</nativeLibrary>
        <initializer>ANXMoPubAdColonyInitializer</initializer>
        <finalizer>ANXMoPubAdColonyFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdColonySupportSimulator.a</nativeLibrary>
        <initializer>ANXMoPubAdColonyInitializer</initializer>
        <finalizer>ANXMoPubAdColonyFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdColonySupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.adcolony.MoPubAdColonySupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.adcolony.MoPubAdColonySupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubAdColonySupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.adcolony.MoPubAdColonySupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.adcolony.MoPubAdColonySupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>
