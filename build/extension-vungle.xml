<extension xmlns="http://ns.adobe.com/air/extension/16.0">
  <id>com.sticksports.nativeExtensions.mopub.vungle</id>
  <versionNumber>0.9.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubVungleSupport.a</nativeLibrary>
        <initializer>ANXMoPubVungleInitializer</initializer>
        <finalizer>ANXMoPubVungleFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubVungleSupportSimulator.a</nativeLibrary>
        <initializer>ANXMoPubVungleInitializer</initializer>
        <finalizer>ANXMoPubVungleFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubVungleSupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.vungle.MoPubVungleSupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.vungle.MoPubVungleSupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubVungleSupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.vungle.MoPubVungleSupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.vungle.MoPubVungleSupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>
