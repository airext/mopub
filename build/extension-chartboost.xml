<extension xmlns="http://ns.adobe.com/air/extension/16.0">
  <id>com.sticksports.nativeExtensions.mopub.chartboost</id>
  <versionNumber>0.9.7</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubChartboostSupport.a</nativeLibrary>
        <initializer>ANXMoPubChartboostInitializer</initializer>
        <finalizer>ANXMoPubChartboostFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubChartboostSupportSimulator.a</nativeLibrary>
        <initializer>ANXMoPubChartboostInitializer</initializer>
        <finalizer>ANXMoPubChartboostFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubChartboostSupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.chartboost.MoPubChartboostSupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.chartboost.MoPubChartboostSupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-x86">
      <applicationDeployment>
        <nativeLibrary>libANXMoPubChartboostSupport.jar</nativeLibrary>
        <initializer>com.sticksports.nativeExtensions.mopub.chartboost.MoPubChartboostSupport</initializer>
        <finalizer>com.sticksports.nativeExtensions.mopub.chartboost.MoPubChartboostSupport</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>
